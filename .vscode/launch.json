    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Node - Launch Program",
            "program": "${file}"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Chrome - Launch file",
            "url": "http://localhost:8080/${relativeFile}",
            "webRoot": "${workspaceFolder}"
        }
    ]
}